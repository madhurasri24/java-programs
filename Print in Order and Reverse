import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for (int i = 1; i <= N; i++) {
            System.out.print(i + " ");
        }
        for (int i = N; i >= 1; i--) {
            System.out.print(i);
            if (i > 1) {
                System.out.print(" ");
            }
        }
    }
}
