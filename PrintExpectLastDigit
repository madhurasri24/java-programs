import java.util.Scanner;

public class AllExceptLastDigit {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int num = sc.nextInt();
        int allExceptLast = num / 10;
        System.out.println("All digits except last: " + allExceptLast);
    }
}
